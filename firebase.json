{
  "hosting": {
    "public": "_site",
    "rewrites": [],
    "headers": [ {
      "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
      }, {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=31536000"
      } ]
      }, {
      "source" : "404.html",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=300"
      } ]
    }, {
      "source" : "**/*.@(css|js|ico)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=2592000"
      } ]
    }, {
      "source" : "**",
      "headers" : [ {
        "key" : "X-Frame-Options",
        "value" : "SAMEORIGIN"
      } ]
    }, {
      "source" : "**",
      "headers" : [ {
        "key" : "X-Content-Type-Options",
        "value" : "nosniff"
      } ]
    } ]
  }
}
